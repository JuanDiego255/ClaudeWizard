.head 5 -  Boolean: bOk
.head 4 +  Actions
.head 5 +  Select Case CurrentTab()
.head 6 -  ! Tab Paso 1
.head 6 +  Case 0
.head 7 -  !
.head 7 +  If dfCuenta
.head 8 -  Set bOk = fciCBAPIPurga.SetCuentaBancaria(dlgCBPurga.dfCuenta)
.head 8 +  If bOk
.head 9 -  Set bOk =fciCBAPIPurga.ValidarCuentaActiva()
.head 9 +  If bOk
.head 10 -  Set bOk = fciCBAPIPurga.fciCuentaBancaria.ObtConsecutivo(CB_CON_FIRM, lnUltimaConcil)
.head 10 -  ! Se le resta uno al consecutivo de Conciliacion para obtener el numero de la ultima conciliacion
.head 10 -  Set lnUltimaConcil = lnUltimaConcil - 1
.head 10 -  !
.head 10 +  If bOk AND (lnUltimaConcil > 0)
.head 11 -  Set bOk = TRUE
.head 10 +  Else If bOk AND (lnUltimaConcil <= 0)
.head 11 -  Set bOk = FALSE
.head 11 -  Call SPSMessageBox(
'No es posible purgar esta Cuenta Bancaria ya que no posee conciliaciones previas.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 10 -  !
.head 9 +  Else
.head 10 -  Set bOk = FALSE
.head 10 -  Call SPSMessageBox(
'No es posible purgar la cuenta ya que no se encuentra activa.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 8 +  Else
.head 9 -  Set bOk=FALSE
.head 9 -  Call SPSDisplayErrorMsg('La Cuenta Bancaria con ese código no existe.', CB_MSG_ERROR )
.head 7 +  Else
.head 8 -  Call SPSMessageBox(
'Debe especificar el código de la Cuenta Bancaria para el proceso.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 8 -  Set bOk = FALSE
.head 7 -  !
.head 7 -  Break
.head 6 -  ! Tab Paso 2
.head 6 +  Case 1
.head 7 +  If dfCodigoConcil >= 0
.head 8 -  !
.head 8 -  ! No permitir purgar a partir de una conciliacion Temporal
.head 8 +  If fciCBAPIPurga.fciCBConcilia.sEstado = CB_CON_TEMP
.head 9 -  Set bOk = FALSE
.head 9 -  Call SPSMessageBox(
'No es posible purgar a partir de una Concilación Bancaria en estado Temporal.  Debe seleccionar una Conciliación en estado En Firme.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 8 +  Else If fciCBAPIPurga.fciCBConcilia.sEstado = CB_CON_FIRM
.head 9 -  Set bOk = TRUE
.head 9 -  ! Validar aspectos de consecutivo de la conciliacion
.head 9 -  !
.head 9 +  If bOk AND (dlgCBPurga.dfCodigoConcil = lnUltimaConcil)
.head 10 -  Set bOk = FALSE
.head 10 -  Call SPSMessageBox(
'No es posible purgar la última Concilación Bancaria en estado En Firme.  Debe seleccionar una Conciliación anterior a ésta.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 9 +  Else If bOk AND (dlgCBPurga.dfCodigoConcil = lnUltimaConcil - 1)
.head 10 -  Set bOk = FALSE
.head 10 -  Call SPSMessageBox(
'No es posible purgar la penúltima Concilación Bancaria en estado En Firme.  Esto provocaría que no se puedan reversar datos muy recientes.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 9 +  Else If bOk AND (dlgCBPurga.dfCodigoConcil > 0) AND (dlgCBPurga.dfCodigoConcil < lnUltimaConcil)
.head 10 -  Set bOk = TRUE
.head 9 +  Else
.head 10 -  Set bOk = FALSE
.head 10 -  Call SPSMessageBox(
'Datos ingresados como parámetro son invalidos.  Consulte la documentación del sistema.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 8 -  !
.head 7 +  Else
.head 8 -  Call SPSMessageBox(
'Debe especificar el código de la Conciliación Bancaria para el proceso.',
CB_MSG_ERROR,
MBF_IconHand, CONTINUAR )
.head 8 -  Set bOk = FALSE
.head 7 -  !
.head 7 -  Break
.head 6 -  ! Tab Paso 3
.head 6 +  Case 2
.head 7 -  Set bOk = TRUE
.head 7 -  Break
.head 5 -  !
.head 5 -  Return bOk
